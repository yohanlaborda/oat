<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="OAT\Question\Domain\Entity\Question" table="question">
        <id name="id" column="id" type="oat_question_id" />
        <field name="text" column="text" type="oat_question_text"/>
        <field name="createdAt" column="created_at" type="oat_question_created_at"/>
        <field name="choices" column="choices" type="oat_question_choices"/>
    </entity>
</doctrine-mapping>
